<Window x:Class="Albion.Network.Interface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="583" Width="582"
        Topmost="True" Closed="Window_Closed">
    <Window.Background>
        <SolidColorBrush Opacity="0.5" Color="White"/>
    </Window.Background>
    <Grid RenderTransformOrigin="0.5,0.5" Width="500" Height="500">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="45"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>
        <Ellipse Fill="#FFF4F4F5" Margin="0" Stroke="Black" OpacityMask="Black"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Center" Height="1" Margin="0" Stroke="Black" VerticalAlignment="Center" Width="500"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Center" Height="500" Margin="0" Stroke="Black" VerticalAlignment="Center" Width="1"/>
        <Grid Name="Points"/>

    </Grid>
</Window>
